{
    "version": 2,
    "builds": [
        {
          "src": "index.html",
          "use": "@vercel/static"
        },
        {
          "src": "loadPages/**/*",
          "use": "@vercel/static"
        },
        {
          "src": "public/**/*",
          "use": "@vercel/static"
        },
        {
          "src": "font/**/*",
          "use": "@vercel/static"
        },
        {
          "src": "project-img/**/*",
          "use": "@vercel/static"
        },
        {
          "src": "package.json",
          "use": "@vercel/static-build"
        }
      ],
    "routes": [
        { "src": "/", "dest": "/index.html" },
        { "src": "/main", "dest": "/public/main/main.html" },
        { "src": "/eaglesMenu/cheer", "dest": "/public/eaglesMenu/cheer/cheer.html" },
        { "src": "/eaglesMenu/gallery", "dest": "/public/eaglesMenu/gallery/gallery.html" },
        { "src": "/eaglesMenu/player", "dest": "/public/eaglesMenu/player/player.html" }
    ]
}